syntax on
set autoindent
set smartindent 
set background="dark"
map <C-N> :set invnumber <CR>
map . :next <CR>
map , :previous <CR>
set noai
set nowrap
set tabstop=4


set showmatch              " brackets/braces that is
set mat=5                  " duration to show matching brace (1/10 sec)
set ignorecase             " ignore case when searching
set visualbell             " shut the fuck up
